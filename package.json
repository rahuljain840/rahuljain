{
  "name": "rahuljain-wp",
  "version": "0.0.1",
  "description": "Source code for rahuljain.work",
  "scripts": {
    "start": "npm-run-all --parallel server-watch client-watch nodemon",
    "build": "npm-run-all --sequential server-build client-build",
    "prod": "cross-env NODE_ENV=production node src/server/index",
    "nodemon": "cross-env NODE_ENV=development nodemon src/server/index",
    "server-watch": "webpack --config webpack/config.server.js",
    "client-watch": "node webpack/dev.server.js",
    "client-build": "webpack --env.prod --config ./webpack/config.client.js",
    "server-build": "webpack --env.prod --config ./webpack/config.server.js",
    "lint:js": "eslint --fix src webpack constants.js",
    "clean": "rm dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "analysis": "webpack --env.prod --env.analysis --config ./webpack/config.client.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rahuljain840/rahuljain.git"
  },
  "keywords": [
    "Rahul"
  ],
  "author": "Rahul Jain",
  "license": "MIT",
  "eslintIgnore": [
    "dist"
  ],
  "dependencies": {
    "antd": "^3.26.7",
    "connected-react-router": "^6.6.1",
    "cookie-parser": "^1.4.4",
    "core-js": "^3.6.4",
    "d3": "^5.15.0",
    "d3-cloud": "^1.2.5",
    "debug": "^4.1.1",
    "express": "^5.0.0-alpha.7",
    "express-rate-limit": "^5.0.0",
    "history": "^4.10.1",
    "http-errors": "^1.7.3",
    "mobile-detect": "^1.4.4",
    "mongoose": "^5.8.7",
    "morgan": "^1.9.1",
    "nodemailer": "^6.4.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-faux-dom": "^4.5.0",
    "react-helmet": "^5.2.1",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-simple-i18n": "^1.2.0",
    "redux": "^4.0.5",
    "regenerator-runtime": "^0.13.3"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/preset-react": "^7.8.3",
    "assets-webpack-plugin": "^3.9.10",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-import": "^1.13.0",
    "compression-webpack-plugin": "^3.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^6.0.3",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "ignore-loader": "^0.1.2",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.5.0",
    "nodemon": "^1.19.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.10",
